{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "shared/*": ["../node_modules/react-native-web/*"] //don't think this is doing anything
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "test/header.test.js"],
  "exclude": ["node_modules"]
}
